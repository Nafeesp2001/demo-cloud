# S3 Bucket & Data File
S3_BUCKET=my-s3-bucket-name            # Replace with your actual S3 bucket name
S3_KEY=sample_data.json                 # Path to your JSON file in S3

# AWS RDS Configuration
RDS_HOST=my-rds-hostname.amazonaws.com  # Replace with your actual RDS host
RDS_PORT=5432                           # Default PostgreSQL port
RDS_DB=mydatabase                       # Replace with your actual database name
RDS_USER=myuser                         # Replace with your database username
RDS_PASS=mypassword                     # Replace with your database password

# AWS Glue Configuration
GLUE_DATABASE=my_glue_database          # Replace with your actual AWS Glue database name
GLUE_TABLE=my_glue_table                # Replace with your actual AWS Glue table name
